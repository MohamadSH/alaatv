{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://packagist.org"
    },
    {
      "packagist": false
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-soap": "*",
    "alaatv/mellat": "1.*",
    "alaatv/payment-gateways": "1.*",
    "alaatv/zarinpal": "1.*",
    "algolia/algoliasearch-client-php": "^2.2",
    "artesaos/seotools": "^0.14.0",
    "cloned/luckybox": "^0.9.0",
    "doctrine/dbal": "^2.9",
    "econea/nusoap": "v0.9.5.3",
    "fideloper/proxy": "^4.0",
    "google/apiclient": "^2.0",
    "greggilbert/recaptcha": "^2.1",
    "guzzlehttp/guzzle": "^6.3",
    "iatstuti/laravel-cascade-soft-deletes": "^1.2",
    "imanghafoori/laravel-terminator": "1.*",
    "intervention/image": "^2.4",
    "intervention/imagecache": "^2.3",
    "jenssegers/agent": "^2.5",
    "kalnoy/nestedset": "^4.3",
    "laravel/framework": "5.8.*",
    "laravel/helpers": "^1.0",
    "laravel/horizon": "^3",
    "laravel/passport": "^7.2",
    "laravel/scout": "^7.1",
    "laravel/telescope": "^2.0",
    "laravel/tinker": "^1.0",
    "laravelcollective/html": "^5.8",
    "league/flysystem-sftp": "~1.0",
    "paulvl/backup": "3.*",
    "propaganistas/laravel-phone": "^4.1",
    "psr/http-factory": "^1.0",
    "santigarcor/laratrust": "5.*",
    "stevebauman/purify": "2.0.*",
    "watson/sitemap": "^2.0",
    "webpatser/laravel-uuid": "^3.0",
    "yab/laravel-scout-mysql-driver": "^2.3",
    "zarinpal/zarinpal": "^1.3"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.6",
    "filp/whoops": "~2.3",
    "fzaninotto/faker": "~1.8",
    "mockery/mockery": "1.2.*",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "symfony/css-selector": "~4.2",
    "symfony/dom-crawler": "~4.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "secure-http":false
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/telescope"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "files": [
      "config/constants.php",
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    },
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
