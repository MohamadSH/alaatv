{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.6.*",
        "laravelcollective/html": "^5.3",
        "greggilbert/recaptcha": "^2.1",
        "doctrine/dbal": "^2.5",
        "iatstuti/laravel-cascade-soft-deletes": "^1.2",
        "jenssegers/agent": "^2.5",
        "laravel/scout": "^4.0",
        "yab/laravel-scout-mysql-driver": "^2.0",
        "econea/nusoap": "v0.9.5.3",
        "algolia/algoliasearch-client-php": "^1.18",
        "santigarcor/laratrust": "5.0.*",
        "eusonlito/laravel-meta": "3.1.*",
        "intervention/image": "^2.4",
        "intervention/imagecache": "^2.3",
        "watson/sitemap": "^2.0",
        "neoxia/laravel-sftp": "^1.0",
        "webpatser/laravel-uuid": "^3.0",
        "cloned/luckybox": "^0.9.0",
        "zarinpal/zarinpal": "^1.3"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0",
        "symfony/css-selector": "~4.0",
        "symfony/dom-crawler": "~4.0",
	    "filp/whoops": "~2.0",
        "barryvdh/laravel-debugbar": "^3.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "config/constants.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
                "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
                "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
